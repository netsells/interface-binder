{
    "name": "netsells/interface-binder",
    "description": "A simple package that allows you to bind interfaces to concrete classes using PHP8 attributes",
    "type": "library",
    "license": "MIT",
     "keywords": [
        "attributes",
        "interface",
        "binder",
        "laravel"
    ],
    "authors": [
        {
            "name": "Adam Hutchison",
            "email": "adam.hutchison@netsells.co.uk"
        },
        {
            "name": "Sam Jordan",
            "email": "sam@netsells.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "netsells/attribute-finder": "dev-task/AAD-8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^6.11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netsells\\InterfaceBinder\\InterfaceBinderServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Netsells\\InterfaceBinder\\": "src",
            "Netsells\\InterfaceBinder\\Tests\\": "tests"
        }
    }
}
